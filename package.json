{
  "name": "@ckken/empsrc",
  "version": "0.3.0",
  "description": "GitHub reference project manager for AI coding agents",
  "type": "module",
  "bin": {
    "empsrc": "./dist/cli.js"
  },
  "scripts": {
    "build": "bun build ./src/cli.ts --outdir ./dist --target bun --format esm",
    "dev": "bun run src/cli.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "ai",
    "coding-agent",
    "reference",
    "github",
    "cli",
    "bun"
  ],
  "author": "Ken <ckken@github>",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.11.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
